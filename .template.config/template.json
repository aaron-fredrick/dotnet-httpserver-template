{
    "$schema": "http://json.schemastore.org/template",
    "author": "Aaron Fredrick",
    "classifications": [
        "Web",
        ".NET",
        "Server"
    ],
    "defaultName" : "HttpServer",
    "identity": "AF.DotnetHttpServer.Template",
    "name": "Dotnet HTTP Server Template",
    "shortName": "af-httpserver",
    "sourceName": "DotnetHttpServerTemplate",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "preferNameDirectory": true,
    "symbols": {
        "templateVersion": {
            "type": "parameter",
            "replaces": "1.0.0-alpha",
            "defaultValue": "1.0.0-beta"
        },
        "authorName": {
            "type": "parameter",
            "replaces": "Aaron Fredrick",
            "defaultValue": "Aaron Fredrick"
        },
        "port": {
            "type": "parameter",
            "replaces": "5566",
            "datatype": "int",
            "defaultValue": "5566"
        },
        "license": {
            "type": "parameter",
            "replaces": "MIT",
            "defaultValue": "MIT"
        }
    }
}